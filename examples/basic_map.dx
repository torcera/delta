type Engine struct {
    cc: int;
}

type Car struct {
    engine: Engine;
}

fn get_car_cc(car: Car) -> int {
    return car.engine.cc;
}

fn get_engine(car: Car) -> Engine {
    return Engine { cc: 450 };
}

fn main() -> void {
    var race_car = Car { engine: Engine { cc: 250 } };
    var race_engine = get_engine(race_car);
    var result = get_car_cc(race_car) + 1 + race_engine.cc;
}
